use eqela:jsh:r4
set version r42

build {
	eqela:jsh cpto build/release src/*.qx
	eqela:jsh makepkq build/release -vendor=eqela -name=slingbuild -version=${version} -platform=all
}

clean {
	eqela:jsh delete build
}
